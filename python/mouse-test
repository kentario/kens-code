#!/usr/bin/env python3

from pynput.mouse import Button, Controller as mouse_controller
from pynput.keyboard import Key, Controller as key_controller
import time
import turtle

keyboard = key_controller()
mouse = mouse_controller()

mouse.position = (0, 0)
mouse.move(2252, 2133)
time.sleep(0.1)
mouse.click(Button.left, 1)
time.sleep(0.1)
mouse.position = (1922, 1505)
time.sleep(1.5)
mouse.click(Button.left, 1)
mouse.position = (1909, 856)
time.sleep(14.5)
mouse.click(Button.left, 2)
time.sleep(1)
mouse.position = (2070, 813)
mouse.click(Button.left, 1)
time.sleep(3)
mouse.click(Button.right, 1)
time.sleep(1)
mouse.position = (1813, 872)
time.sleep(1)
mouse.click(Button.left, 1)
time.sleep(3)
keyboard.press('w')
time.sleep(1)
keyboard.release('w')
keyboard.press('a')
time.sleep(0.1)
keyboard.release('a')
keyboard.press('c')
keyboard.release('c')
mouse.click(Button.right, 1)
time.sleep(0.3)
mouse.position = (1867, 933)
mouse.click(Button.left, 1)
time.sleep(0.5)
keyboard.press('e')
keyboard.release('e')
keyboard.press('a')
time.sleep(0.9)
keyboard.release('a')
mouse.click(Button.right, 1)
time.sleep(0.3)
mouse.position = (1867, 933)
mouse.click(Button.left, 1)
time.sleep(0.5)
keyboard.press('e')
keyboard.release('e')
